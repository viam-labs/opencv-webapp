{
  "module_id": "viam:opencv-webapp",
  "visibility": "public",
  "url": "https://github.com/viam-labs/opencv-webapp",
  "description": "Exposes OpenCV calibration files through a web interface",
  "models": [
    {
      "api": "rdk:service:generic",
      "model": "viam:opencv-webapp:webapp"
    }
  ],
  "entrypoint": "run.sh",
  "build": {
    "setup": "make setup",
    "build": "make build",
    "path": "module.tar.gz",
    "arch": [
      "linux/amd64",
      "linux/arm64"
    ]
  },
  "applications": [
    {
      "name": "opencv-calibration-viewer",
      "type": "single_machine",
      "entrypoint": "dist/index.html",
      "logoPath": "dist/viam.png",
      "customizations": {
        "heading": "Hand-Eye Calibration Data Viewer",
        "subheading": "Select a machine to browse calibration passes"
      }
    }
  ]
}
